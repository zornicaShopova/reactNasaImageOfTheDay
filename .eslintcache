      <div className=\"row\">\n                <div className=\"col\">\n                <Card className=\"mb-3\">\n                    <div className=\"row\">\n                    <div className=\"col-3\">\n                        {getMovieImage()}\n                    </div>\n                    <div className=\"col\">\n                        <CardBody>\n                            <CardTitle tag=\"h5\" className=\"text-truncate\">{movie.title}</CardTitle>\n                            <CardSubtitle>{movie.release_date}</CardSubtitle>\n                            <CardText className=\"movie-description\">{movie.overview}</CardText>\n                        </CardBody>\n                    </div>\n                    </div>\n                </Card>\n                </div>\n            </div>\n        </div>\n   \n    </>\n}\nexport default MovieDetails;","C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\pages\\Movies.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\layouts\\Default.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\redux\\action-types.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\redux\\actions.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\constants.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\carousel\\Carousel.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\contact\\ContactForm.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\movie\\MovieGridItem.js",["208"],"import {\n    Card, \n    CardImg,\n    CardText,\n    CardBody,\n    CardTitle,\n    Button,\n  } from 'reactstrap';\nimport {Link} from 'react-router-dom';\n\nimport constants from '../../constants';\n\nconst MovieGridItem = props => {\n\n    const getMovieImage = () => {\n      if(!props.poster_path){\n        return null;\n      }\n      return <CardImg \n        top \n        width=\"100%\" \n        src={`${constants.posterPath}${props.poster_path}`} \n      />\n    }\n\n    return (\n      <div className=\"col-md-3\">\n        <Card className=\"mb-3\">\n          {getMovieImage()}\n          <CardBody>\n            <CardTitle tag=\"h5\" className=\"text-truncate\">{props.title}</CardTitle>\n            <CardText className=\"movie-description\">{props.overview}</CardText>\n            <Link to={`/movie/${props.id}`} className=\"btn btn-secondary\">\n              Movie details\n            </Link>\n          </CardBody>\n        </Card>\n      </div>\n    );\n  };\n  \nexport default MovieGridItem;\n  ","C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\movie\\MovieListItem.js",["209"],"import {\n    Card, \n    CardImg,\n    CardText,\n    CardBody,\n    CardTitle,\n    Button,\n  } from 'reactstrap';\nimport {Link} from 'react-router-dom';\nimport constants from '../../constants';\n\nconst MovieListItem = props => {\n\n    const getMovieImage = () => {\n      if(!props.poster_path){\n        return null;\n      }\n      return <CardImg \n        top \n        width=\"100%\" \n        src={`${constants.posterPath}${props.poster_path}`} \n      />\n    }\n\n    return (\n      <div className=\"col-12\">\n          <Card className=\"mb-3\">\n            <div className=\"row\">\n              <div className=\"col-3\">\n                {getMovieImage()}\n              </div>\n              <div className=\"col\">\n                <CardBody>\n                  <CardTitle tag=\"h5\" className=\"text-truncate\">{props.title}</CardTitle>\n                  <CardText className=\"movie-description\">{props.overview}</CardText>\n                  <Link to={`/movie/${props.id}`} className=\"btn btn-secondary\">\n                      Movie details\n                  </Link>\n                </CardBody>\n              </div>\n            </div>\n          </Card>\n      </div>\n    );\n  };\n  \nexport default MovieListItem;\n  ","C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\filters\\Filters.js",["210"],"import React, {useEffect} from 'react';\nimport SelectYear from './SelectYear';\nimport SelectGenres from './SelectGenres';\nimport {Button} from 'reactstrap';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { getMovies } from '../../redux/actions';\n\nconst Filters = (props) => {\n\n    const selectedYear = useSelector(state => state.selectedFilters.selectedYear);\n    const selectedGenres = useSelector(state => state.selectedFilters.selectedGenres);\n\n    const dispatch = useDispatch();\n    \n    const loadMovies = () => {\n        dispatch(getMovies({\n            year: selectedYear,\n            page: 1,\n            with_genres: selectedGenres.toString(),\n        }));\n    }\n\n    useEffect(() => {\n        loadMovies();\n    }, []);\n\n    return (\n        <>\n            <div className=\"row\">\n                <div className=\"col-md-4\">\n                    <SelectYear/>\n                </div>\n                <div className=\"col-md-4\">\n                    <SelectGenres/>\n                </div>\n            </div>\n            <div className=\"row mb-3\">\n                <div className=\"col\">\n                    <Button color=\"primary\" onClick={loadMovies}>Find</Button>\n                </div>\n            </div>\n        </>\n    );\n}\n\nexport default Filters;","C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\navbar\\Navbar.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\network\\network-client.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\ClassComponent.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\FunctionalComponent.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\filters\\SelectYear.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\filters\\SelectGenres.js",["211"],"import React, { useEffect } from 'react';\nimport { FormGroup, Label, Input } from 'reactstrap';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { getGenres, setSelectedGenres } from '../../redux/actions';\nimport { createSelector } from 'reselect';\nconst SelectGenres = (props) => {\n\n    const dispatch = useDispatch();\n\n    const genres = useSelector(state => state.genres);\n\n    useEffect(() => {\n        dispatch(getGenres({\n            language: 'bg-BG',\n        }));\n    }, []);\n\n    const selectedFilters = useSelector(state => state.selectedFilters)\n\n    const inputOnChange = (event) => {\n        const isChecked = event.target.checked;\n        const genreId = event.target.value;\n        let currentGenres = selectedFilters.selectedGenres;\n        console.log('inputOnChange', isChecked, genreId);\n        if(isChecked){\n            currentGenres.push(genreId);\n        }else {\n            currentGenres = currentGenres.filter(id => id !== genreId);\n        }\n        dispatch(setSelectedGenres(currentGenres));\n    }\n\n    const getId = (_, id) => id;\n    const getSelectedGenres = state => state.selectedGenres;\n\n    const isGenreSelected = createSelector(\n        getSelectedGenres,\n        getId,\n        (genres, id) => {\n            return genres.includes(id)\n        }\n    )\n\n    const isChecked = (genreId) => {\n        const isChecked = isGenreSelected(selectedFilters, genreId);\n        return isChecked;\n    }\n\n    const getGenresCheckboxes = () => {\n        const genresCheckboxes = genres.map(genre => {\n            return <FormGroup check inline key={genre.id}>\n                <Label check>\n                    <Input type=\"checkbox\" \n                    checked={isChecked(genre.id.toString())} \n                    value={genre.id}\n                    onChange={inputOnChange}/> \n                    {genre.name}\n                </Label>\n            </FormGroup>\n        })\n        return genresCheckboxes;\n    }\n\n    return (\n        <FormGroup>\n            <Label>Select genres</Label>\n            <div>\n                {getGenresCheckboxes()}\n            </div>\n        </FormGroup>\n       \n    );\n}\n\nexport default SelectGenres;","C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\pages\\NasaImages.js",["212"],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\nasa_constant.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\network\\network-nasa-client.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\nasaData\\nasaImage.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\filters\\SelectDay.js",[],"C:\\ZORI\\PROJECT\\fmi projects\\client-js-react-master\\src\\components\\about\\About.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\index.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\App.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\reportWebVitals.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\redux\\reducers.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\pages\\Contact.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\pages\\NasaImages.js",["213"],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\pages\\Home.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\layouts\\Default.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\redux\\action-types.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\components\\filters\\SelectDay.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\components\\carousel\\Carousel.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\components\\nasaData\\nasaImage.js",[],"C:\\ZORI\\PROJECT\\repoNasaReact\\reactNasaImageOfTheDay\\src\\compone